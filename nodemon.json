{
    "restartable": "rs",
    "ignore": [
        ".git",
        "node_modules/**/node_modules"
    ],
    "execMap": {
        "js": "node"
    },
    "verbose": true,
    "exec": "node web/backend/app.js",
    "watch": [
        "web/backend/",
        "web/frontend/"
    ],
    "ext": "js,html"
}